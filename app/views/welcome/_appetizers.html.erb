<div class="row">
  <% @appetizers.each do |item| %>
      <div class="col-md-4 item-entry">
        <div class="item-title">
          <%= item.title %>
        </div>
        <div class="pull-right">
          <%= price_correction(item) %>
        </div>
      </div>
  <% end %>
</div>
<div>
  <div id="pupu">
    Pu Pu Platter &nbsp 8.95 per person (min 2)
  </div>
  <div id="pupu-desc">
    Egg rolls, fried shrimp, fried wontons, paper wrapped chicken, and beef skewers arranged with a mini grill
  </div>
</div>
